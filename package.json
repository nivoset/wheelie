{
  "name": "wheelie",
  "version": "1.0.0",
  "type": "module",
  "description": "Discord carpool bot with web dashboard",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently \"npm run dev:build\" \"npm run dev:server\" \"npm run dev:bot\" \"cd dashboard && pnpm dev\"",
    "dev:build": "tsc --watch --preserveWatchOutput",
    "dev:server": "node --loader ts-node/esm dist/dashboard/server.js",
    "dev:bot": "node --watch --enable-source-maps dist/bot.js",
    "dev:dashboard": "node --watch --enable-source-maps dist/dashboard/server.js",
    "build": "concurrently \"tsc\" \"cd dashboard && npm run build\"",
    "test": "node --test dist/__tests__/*.test.js",
    "test:watch": "node --test --watch dist/__tests__/*.test.js",
    "test:coverage": "node --test --coverage dist/__tests__/*.test.js",
    "dashboard": "node dist/dashboard/server.js",
    "dashboard:build": "tsc && cd dashboard && npm run build"
  },
  "dependencies": {
    "@types/sequelize": "^4.28.20",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "luxon": "^3.4.3",
    "module-alias": "^2.2.3",
    "node-geocoder": "^4.2.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "sequelize": "^6.37.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.12",
    "@types/luxon": "^3.6.2",
    "@types/node": "^20.11.19",
    "@types/node-geocoder": "^3.24.5",
    "@types/passport": "^1.0.16",
    "@types/passport-discord": "^0.1.4",
    "@types/sqlite3": "^3.1.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "~": "dist"
  }
}